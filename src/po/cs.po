#: ../meh/ui/text.py:156
msgid "Report Bug"
msgstr "Nahlásit chybu"

#: ../meh/ui/text.py:157
msgid "Debug"
msgstr "Debug"

#: ../meh/ui/text.py:158
msgid "Quit"
msgstr "Ukončit"

#: ../meh/ui/text.py:163
msgid "What do you want to do now?"
msgstr "Jak chcete pokračovat?"

#: ../meh/ui/text.py:171
msgid "Please make your choice from above: "
msgstr "Vyberte jednu z výše uvedených voleb:"

