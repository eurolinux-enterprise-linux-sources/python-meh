msgid ""
"Local storage\n"
"Bugzilla\n"
"Remote server (scp)"
msgstr ""
"Lokalno skladište\n"
"Bugzilla\n"
"Udaljeni server (scp)"

#: tmp/exnSave.glade.h:6
msgid "Please choose a destination for saving your traceback."
msgstr "Izaberite odredište za čuvanje povratnog traga."

#: tmp/exnSave.glade.h:7
msgid "Save"
msgstr "Sačuvaj"

#: tmp/exnSave.glade.h:8
msgid "Select A File"
msgstr "Izaberite datoteku"

#: tmp/exnSave.glade.h:9
msgid "_Host (host:port)"
msgstr "_Domaćin (domaćin:port)"

#: tmp/exnSave.glade.h:10
