#. short (one-char) answer meaning "yes"
#: ../meh/ui/text.py:70
msgid "y"
msgstr "i"

#. short (one-char) answer meaning "no"
#: ../meh/ui/text.py:73
msgid "n"
msgstr "n"

#: ../meh/ui/text.py:140
msgid "Save exception"
msgstr "Kivétel mentése"

#: ../meh/ui/text.py:153 tmp/exception-dialog.glade.h:1
msgid "An unknown error has occurred"
msgstr "Egy ismeretlen hiba történt"

#: ../meh/ui/text.py:156
msgid "Report Bug"
msgstr "Hiba bejelentése"

#: ../meh/ui/text.py:157
msgid "Debug"
msgstr "Hibakeresés"

#: ../meh/ui/text.py:158
msgid "Quit"
msgstr "Kilépés"

#: ../meh/ui/text.py:163
msgid "What do you want to do now?"
msgstr "Mi szeretne most tenni?"

#: ../meh/ui/text.py:171
msgid "Please make your choice from above: "
msgstr "Kérem válasszon a fentiekből:"

#: ../meh/ui/text.py:189
msgid "Hit ENTER to continue"
msgstr "Nyomjon ENTER-t a folytatáshoz"

#: ../meh/ui/text.py:203
#, python-format
msgid "Are you sure you want to exit? [%(yes)s/%(no)s]"
msgstr "Biztos benne hogy ki szeretne lépni? [%(yes)s/%(no)s]"

#: ../meh/ui/gui.py:150
msgid "_Exit"
msgstr "Kilépés"

#: tmp/exception-dialog.glade.h:2
msgid ""
"This program has encountered an unknown error. You may\n"
"report the bug below or quit the program."
msgstr "A program egy ismeretlen hibába ütközött. Jelentheti\na hibát alább vagy kiléphet a programból."

#: tmp/exception-dialog.glade.h:4
msgid "_Report Bug"
msgstr "Hiba bejelentése"

#: tmp/exception-dialog.glade.h:5
msgid "_Quit"
msgstr "Kilépés"

#: tmp/exception-dialog.glade.h:6
msgid "The output below may help determine the cause of the error:"
msgstr "Az alábbi kimenet esetleg segíthet meghatározni mi okozta a hibát:"

#: tmp/exception-dialog.glade.h:7
msgid "'Debug' may take you to tty1."
msgstr "'Debug' a tty1 terminálra tehet ki."

#: tmp/exception-dialog.glade.h:8
msgid "_Debug"
msgstr "_Debug"

#: tmp/exception-dialog.glade.h:9
msgid "More _info..."
msgstr "További _info..."

#: tmp/exception-dialog.glade.h:10
msgid "No additional info available."
msgstr "Nincs elérhető további információ."
