msgid ""
"Local storage\n"
"Bugzilla\n"
"Remote server (scp)"
msgstr ""
"Almacenamiento local\n"
"Bugzilla\n"
"Servidor remoto (scp)"

#: tmp/exnSave.glade.h:6
msgid "Please choose a destination for saving your traceback."
msgstr "Por favor elija un destino donde almacenar la descripción de su error."

#: tmp/exnSave.glade.h:7
msgid "Save"
msgstr "Guardar"

#: tmp/exnSave.glade.h:8
msgid "Select A File"
msgstr "Elegir un archivo"

#: tmp/exnSave.glade.h:9
msgid "_Host (host:port)"
msgstr "_Equipo (nombre:puerto)"

#: tmp/exnSave.glade.h:10
